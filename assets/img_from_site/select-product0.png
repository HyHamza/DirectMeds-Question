<script>
document.addEventListener("DOMContentLoaded", function () {
  const popup = document.getElementById("video-popup");
  const playerContainer = document.getElementById("video-popup-player");
  const closeBtn = document.querySelector(".video-popup-close");
  const overlay = document.querySelector(".video-popup-overlay");

  function getEmbedCode(url) {
    if (!url) return `<p style="color:white; padding: 20px;">Invalid video URL.</p>`;
    url = url.trim();

    // YouTube
    if (url.includes("youtube.com/embed") || url.includes("youtube.com/watch") || url.includes("youtu.be")) {
      let embedUrl = url;

      if (url.includes("watch")) {
        const videoId = new URL(url).searchParams.get("v");
        embedUrl = `https://www.youtube.com/embed/${videoId}`;
      }

      if (url.includes("youtu.be")) {
        const videoId = url.split("/").pop();
        embedUrl = `https://www.youtube.com/embed/${videoId}`;
      }

      embedUrl += (embedUrl.includes("?") ? "&" : "?") + "autoplay=1&rel=0";

      return `<iframe src="${embedUrl}" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>`;
    }

    // Vimeo
    if (url.includes("vimeo.com")) {
      const vimeoId = url.split("/").pop();
      return `<iframe src="https://player.vimeo.com/video/${vimeoId}?autoplay=1"
              frameborder="0" allow="autoplay; fullscreen" allowfullscreen></iframe>`;
    }

    // MP4 or WebM
    if (url.endsWith(".mp4") || url.endsWith(".webm")) {
      return `<video controls autoplay>
                <source src="${url}" type="video/mp4">
                Your browser does not support the video tag.
              </video>`;
    }

    return `<p style="color:white; padding: 20px;">Unsupported video format or URL.</p>`;
  }

  function openPopup(videoUrl) {
    const embedHTML = getEmbedCode(videoUrl);
    playerContainer.innerHTML = embedHTML;
    popup.style.display = "flex";
  }

  function closePopup() {
    playerContainer.innerHTML = "";
    popup.style.display = "none";
  }

  document.addEventListener("click", function (e) {
    const button = e.target.closest(".play-button");
    if (button && button.dataset.video) {
      openPopup(button.dataset.video);
    }
  });

  closeBtn.addEventListener("click", closePopup);
  overlay.addEventListener("click", closePopup);
});
</script>


