<script>
    document.addEventListener("DOMContentLoaded", () => {
        const counters = document.querySelectorAll('.number');
        let observer = new IntersectionObserver((entries, observer) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    let target = entry.target;
                    let targetValue = parseInt(target.getAttribute('data-target'));
                    let startValue = 0;
                    let duration = 700; // 2 seconds
                    let increment = targetValue / (duration / 16);

                    let updateCounter = () => {
                        startValue += increment;
                        if (startValue < targetValue) {
                            target.textContent = Math.floor(startValue);
                            requestAnimationFrame(updateCounter);
                        } else {
                            target.textContent = targetValue;
                        }
                    };
                    updateCounter();
                    observer.unobserve(target); // Stop observing once counted
                }
            });
        }, { threshold: 0.5 }); // Trigger when 50% is visible

        counters.forEach(counter => observer.observe(counter));
    });
</script>
